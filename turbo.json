{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": []
    },
    "lint": {},
    "lint:fix": {},
    "typecheck": {
      "cache": false
    },
    "build": {
      "outputs": [
        "dist/**",
        "lib/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "docs#build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "@adhese/sdk#build"
      ]
    },
    "prepareRelease": {
      "outputs": [
        "dist/**",
        "lib/**"
      ],
      "dependsOn": [
        "^test",
        "^lint",
        "^typecheck",
        "^prepareRelease"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false,
      "outputLogs": "errors-only"
    }
  }
}
