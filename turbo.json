{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "outputMode": "errors-only"
    },
    "lint:fix": {
      "outputMode": "errors-only"
    },
    "typecheck": {
      "outputMode": "errors-only"
    },
    "build": {
      "outputs": ["dist/**", "lib/**"],
      "dependsOn": ["^build", "lint", "test", "typecheck"],
      "outputMode": "new-only"
    },
    "@adhese/sdk#build": {
      "outputs": ["dist/**", "lib/**"],
      "dependsOn": ["^build", "lint", "test", "typecheck", "core#build"],
      "outputMode": "new-only"
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false,
      "outputMode": "errors-only"
    }
  }
}
