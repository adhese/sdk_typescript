{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {},
    "lint:fix": {},
    "typecheck": {},
    "build": {
      "outputs": ["dist/**", "lib/**"],
      "dependsOn": ["^build"]
    },
    "prepareRelease": {
      "outputs": ["dist/**", "lib/**"],
      "dependsOn": ["^prepareRelease"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false,
      "outputMode": "errors-only"
    }
  }
}
