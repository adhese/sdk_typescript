{
  "name": "@adhese/sdk-react",
  "type": "module",
  "version": "0.8.0",
  "description": "Adhese SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adhese/sdk_typescript.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsup src/index.ts --dts-only --minify --format esm",
    "lint": "eslint . --ignore-pattern **/*.md/**/*",
    "lint:fix": "eslint --fix . --ignore-pattern **/*.md/**/*",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepareRelease": "npm run build"
  },
  "peerDependencies": {
    "@adhese/sdk": "^0",
    "react": ">=16.13",
    "react-dom": ">=16.13"
  },
  "dependencies": {
    "remeda": "^1.61.0",
    "use-deep-compare-effect": "^1.8.1"
  },
  "devDependencies": {
    "react": "^18.0.2",
    "react-dom": "^18.2.0"
  }
}
