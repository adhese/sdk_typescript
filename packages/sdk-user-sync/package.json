{
  "name": "@adhese/sdk-user-sync",
  "type": "module",
  "version": "0.1.0",
  "description": "Adhese SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adhese/sdk_typescript.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsup src/index.ts --dts-only --minify --format esm",
    "lint": "eslint . --ignore-pattern **/*.md/**/*",
    "lint:fix": "eslint --fix . --ignore-pattern **/*.md/**/*",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepareRelease": "npm run build"
  },
  "peerDependencies": {
    "@adhese/sdk": ">= 0.20 < 1"
  },
  "dependencies": {
    "@adhese/sdk-shared": "^0.9.0"
  }
}
