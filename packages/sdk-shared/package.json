{
  "name": "@adhese/sdk-shared",
  "type": "module",
  "version": "0.1.1",
  "description": "Adhese SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adhese/sdk_typescript.git"
  },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsup src/index.ts --dts-only --minify --format esm",
    "dev": "vitest --silent",
    "test": "vitest --run --coverage --silent",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "prepareRelease": "npm run build"
  }
}
