{
  "name": "@adhese/sdk-shared",
  "type": "module",
  "version": "1.1.0",
  "description": "Adhese SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adhese/sdk_typescript.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./validators": {
      "types": "./dist/validators.d.ts",
      "import": "./dist/validators.js",
      "require": "./dist/cjs/validators.cjs"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vitest --silent",
    "dev:verbose": "vitest",
    "test": "vitest --run --coverage --silent",
    "test:verbose": "vitest --run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "prepareRelease": "npm run build"
  },
  "peerDependencies": {
    "@adhese/sdk": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "@adhese/sdk": {
      "optional": true
    }
  },
  "dependencies": {
    "@vue/runtime-core": "^3.2.29",
    "remeda": "^2.2.1",
    "zod": "^3.23.8"
  }
}
