{
  "name": "@adhese/sdk-devtools",
  "type": "module",
  "version": "0.7.1",
  "description": "Adhese SDK Devtools",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adhese/sdk_typescript.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsup src/index.ts --dts-only --minify --format esm",
