{
  "name": "@adhese/sdk-gambit",
  "type": "module",
  "version": "1.0.0",
  "description": "Adhese SDK",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adhese/sdk_typescript.git"
  },
  "sideEffects": false,
  "main": "./dist/cjs/gambit.cjs",
  "module": "./dist/gambit.js",
